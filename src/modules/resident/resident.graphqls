type Resident {
    id: Int!
    name: String!
    home: Home
}

extend type Query {
    resident(id: Int!): Resident
    allResidents: [Resident!]
}

extend type Mutation {
    createResident(name: String!, homeId: Int): Resident!
    updateResident(id: Int!, name: String!, homeId: Int): Resident!
}